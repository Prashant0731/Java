package com.practice.StringBuilder_V1_5;

public class ExampleSync {

	public static void main(String[] args) {
		
		StringBuffer sb = new StringBuffer(" StringBuffer : 1 ");
		sb.append(" StringBuffer : 2 ");
		System.out.println(sb);
		
		
		StringBuilder sb1 = new StringBuilder(" StringBuilder : 1 ");
		sb1.append(" StringBuilder : 2 ");
		System.out.println(sb1);
		System.out.println(sb1);
	}
}
